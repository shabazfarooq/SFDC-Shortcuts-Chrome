{
  "name": "Test SFDC Setup shortcut",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>", "https://*.salesforce.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {
    "goToSfdcSetup": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Ctrl+Shift+S"
      },
      "description": "Loads the Salesforce setup page"
    },
    "goToSfdcPages": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Ctrl+Shift+P"
      },
      "description": "Loads the Salesforce pages"
    },
    "goToSfdcClasses": {
      "suggested_key": {
        "default": "Ctrl+Shift+C",
        "mac": "Ctrl+Shift+C"
      },
      "description": "Loads the Salesforce classes"
    }
  },
  "permissions": [
    "tabs"
  ]
}